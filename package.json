{
  "name": "@nfq/svg-to-react",
  "description": "Package to batch create react components from svg images",
  "author": ".NFQ | Christoph Kruppe",
  "version": "2.0.5",
  "keywords": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nfqde/nfq-svg-to-react.git"
  },
  "bugs": {
    "url": "https://github.com/nfqde/nfq-svg-to-react/issues"
  },
  "homepage": "https://github.com/nfqde/nfq-svg-to-react#readme",
  "template": {
    "version": "1.0.0",
    "name": "nfq-npm-modules-template",
    "path": "https://github.com/nfqde/nfq-npm-modules-template.git"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "bin": {
    "svgToReact": "./dist/index.js"
  },
  "types": "types/index.d.ts",
  "sideEffects": [
    "cypress/**"
  ],
  "scripts": {
    "build": "rollup -c && pnpm tsc",
    "deploy": "standard-version && git push --follow-tags origin main && pnpm publish",
    "lint": "pnpm eslint ./src --ext='.js, .jsx, .ts, .tsx'",
    "lint:report": "pnpm eslint --output-file eslint_report.json --format json --ext='.js, .jsx, .ts, .tsx' ./src",
    "merge:files": "pnpm mochawesome-merge \"./cypress/reports/mochawesome*.json\" > \"./cypress/reports/index.json\"",
    "prepublishOnly": "pnpm run build",
    "test": "pnpm cross-env CYPRESS_INTERNAL_BROWSER_CONNECT_TIMEOUT=300000 pnpm cypress run --component && pnpm merge:files",
    "test:open": "pnpm cross-env NODE_ENV=test pnpm cypress open --component"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/eslint-parser": "^7.21.8",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@babel/plugin-syntax-import-assertions": "^7.20.0",
    "@babel/plugin-transform-typescript": "^7.21.3",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@cypress/code-coverage": "^3.10.4",
    "@cypress/webpack-dev-server": "^3.4.1",
    "@lopatnov/rollup-plugin-uglify": "^2.1.5",
    "@nfq/eslint-config": "^2.3.0",
    "@nfq/eslint-plugin": "^0.6.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@types/command-line-args": "^5.2.0",
    "@types/command-line-usage": "^5.0.2",
    "@types/cypress__code-coverage": "^3.10.0",
    "@types/node": "^20.1.6",
    "@types/react": "^18.2.6",
    "@types/svg-parser": "^2.0.3",
    "@types/webpack": "^5.28.1",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "babel-loader": "^9.1.2",
    "babel-plugin-istanbul": "^6.1.1",
    "babel-plugin-styled-components": "^2.1.1",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "cypress": "^12.12.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-array-func": "^3.1.8",
    "eslint-plugin-better-styled-components": "^1.1.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^40.0.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-no-unsanitized": "^4.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-perf-standard": "^1.0.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-hooks-ssr": "^0.1.5",
    "eslint-plugin-redos": "4.4.5",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-sort-destructure-keys": "^1.5.0",
    "html-webpack-plugin": "^5.5.1",
    "mocha": "^10.2.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.21.8",
    "rollup-plugin-add-shebang": "^0.3.1",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-copy": "^3.4.0",
    "terser": "^5.17.4",
    "typescript": "^5.0.4",
    "webpack": "^5.82.1",
    "webpack-cli": "^5.1.1",
    "webpack-dev-server": "4.15.0"
  },
  "externals": {
    "command-line-args": "^5.2.1",
    "command-line-usage": "^6.1.3",
    "eslint": "^8.35.0",
    "svgo": "^3.0.2",
    "svgson": "^5.2.1"
  },
  "standard-version": {
    "scripts": {
      "prebump": "nfqBump"
    }
  },
  "dependencies": {
    "command-line-args": "^5.2.1",
    "command-line-usage": "^7.0.1",
    "eslint": "^8.40.0",
    "svgo": "^3.0.2",
    "svgson": "^5.2.1"
  }
}
